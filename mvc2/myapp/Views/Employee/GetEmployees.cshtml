@model myapp.Models.Employee;
@{
    ViewData["Title"] = "Employee";
    List<Employee> employees = ViewData["empdata"] as List<Employee>;
}

<div className="container">
    <table calssName="table">
        <thead>
            <tr>
                <th scope="col">Id</th>
                <th scope="col">Name</th>
                <th scope="col">Delete</th>
                <th scope="col">Edit</th>
            </tr>
        </thead>
        <tbody>
            @foreach (Employee item in employees)
            {
                <tr scope="row">
                    <td>@item.Id</td>
                    <td>@item.Name</td>
                    <td><a href="/Employee/Delete/@item.Id">delete</a></td>
                    <td><a href="/Employee/Edit/@item.Id">Edit</a></td>
                </tr>
            }
        </tbody>

       

    </table>
</div>